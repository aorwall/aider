def score(word):
    # Initialize a dictionary to map letters to their scores
    letter_scores = {"a": 1, "c": 3, "b": 3, "e": 1, "d": 2, "g": 2,
                     "f": 4, "i": 1, "h": 4, "k": 5, "j": 8, "m": 3,
                     "l": 1, "o": 1, "n": 1, "q": 10, "p": 3, "s": 1,
                     "r": 1, "u": 1, "t": 1, "w": 4, "v": 4, "y": 4,
                     "x": 8, "z": 10}

    # Create a variable to store the word's score
    total_score = 0

    # Iterate through each letter in the word and add its score to the total score
    for char in word:
        if char in letter_scores:
            total_score += letter_scores[char]

    return total_score


