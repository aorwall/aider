def steps(number):
    # Check if the number is less than or equal to 0
    if number <= 0:
        raise ValueError("Number must be greater than 0.")
    
    count = 0
    while number != 1:
        if number % 2 == 0:
            number //= 2
        else:
            number = (3*number) + 1
        count += 1
    return count

