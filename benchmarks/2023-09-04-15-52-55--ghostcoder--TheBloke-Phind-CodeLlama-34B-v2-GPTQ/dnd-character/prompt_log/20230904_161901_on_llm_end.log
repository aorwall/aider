import random

class Character:
    def __init__(self):
        self._abilities = {}
        self.generate_abilities()
        self.calculate_hitpoints()

    def generate_abilities(self):
        for _ in range(6):
            roll = sorted([random.randint(1, 6) for _ in range(4)])[-3:]
            score = sum(roll)
            self._abilities[list(self._abilities.keys())[-1]] = score

    def calculate_hitpoints(self):
        const = self._abilities["constitution"]
        self.hitpoints = max(10 + ((const - 10) // 2), 1)

    @property
    def strength(self):
        return self._abilities.get('strength', None)

    @property
    def dexterity(self):
        return self._abilities.get('dexterity', None)

    @property
    def constitution(self):
        return self._abilities.get('constitution', None)

    @property
    def intelligence(self):
        return self._abilities.get('intelligence', None)

    @property
    def wisdom(self):
        return self._abilities.get('wisdom', None)

    @property
    def charisma(self):
        return self._abilities.get('charisma', None)

