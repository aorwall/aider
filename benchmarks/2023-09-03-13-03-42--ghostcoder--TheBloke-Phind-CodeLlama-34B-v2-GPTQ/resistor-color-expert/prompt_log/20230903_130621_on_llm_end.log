        tolerance=20
    else:
        value += str(color_dict[colors[0]]*10+color_dict[colors[1]])
        multiplier = color_dict[colors[2]]
        tolerance=25
    
    return "{0} {1}Ohms {2}%".format(float(value)*pow(10,multiplier),("kilo","mega")[int(multiplier/3)-1],tolerance)
