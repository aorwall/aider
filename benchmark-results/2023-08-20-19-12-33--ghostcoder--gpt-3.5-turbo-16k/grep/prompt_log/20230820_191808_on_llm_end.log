    if '-n' in flags:
        # If the -n flag is present, prepend the line number and a colon to the line
        line = f"{line_number}:{line}"
    if '-l' in flags:
        # If the -l flag is present, return only the file name
        return file
    else:
        # Prepend the file name and a colon to the line
        return f"{file}:{line}"
